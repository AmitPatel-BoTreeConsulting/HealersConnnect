- unless target.nil?
  - if target.errors.any?
    .row.flash_msg
      - target.errors.full_messages.each do |msg|
        .text-center.alert.alert-danger.margin-bottom-10
          %button.close{'data-dismiss' => 'alert', type: 'button'} &times;
          %i{ class: 'icon-remove' }
          = msg
